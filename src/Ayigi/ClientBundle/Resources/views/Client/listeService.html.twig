{% extends 'layout_ayigi.html.twig' %}

{% block contenu %}

  <section class="title">
    <div class="container">
      <div class="row-fluid">
        <div class="span6">
          <h1>Liste des services</h1>
        </div>
        <div class="span6">
          <ul class="breadcrumb pull-right">
            <li><a href="index.html">Home</a> <span class="divider">/</span></li>
            <li><a href="#">Pages</a> <span class="divider">/</span></li>
            <li class="active">Terms of Use</li>
          </ul>
        </div>
      </div>
    </div>
  </section>
  <!-- / .title -->

  <section id="terms-use" class="container">
    <div class="table-responsive">
      <table class="table table-striped">
        <thead>
        <tr>
          <th>id</th>
          <th>Nom</th>
          <th>Description</th>
          <th>Pays</th>

        </tr>
        </thead>
        <tbody>
        {% for service in services %}
          <tr>
            <td>{{ service.id }} </td>
            <td>{{ service.nom }} </td>
            <td>{{ service.description }} </td>
            <td>
                {% for e in service.etablissements %}
                  <ol>{{ e.nom }}</ol>
                {% endfor %}
            </td>
          </tr>
        {% endfor %}

        </tbody>
      </table>
    </div>

  </section>
{% endblock %}


